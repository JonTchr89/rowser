{
  "name": "rowser",
  "version": "0.1.0",
  "description": "A smart browser/os/bot detector based on User Agent string parsing",
  "main": "index.js",
  "scripts": {
    "benchmark": "node ./benchmark/index.js",
    "build": "rollup -c",
    "build:open": "node ./scripts/index-page-open.js",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha ./__tests__",
    "coverage:open": "node ./scripts/coverage-report-open.js",
    "lint": "",
    "stats": "cloc . --exclude-dir=node_modules,tmp,.git",
    "test": "./node_modules/.bin/mocha ./__tests__",
    "test:unit": "./node_modules/.bin/mocha ./__tests__/unit.js",
    "test:detection": "./node_modules/.bin/mocha ./__tests__/detection"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/everget/rowser.git"
  },
  "keywords": [
    "browser",
    "detector",
    "agent",
    "parser"
  ],
  "author": "Alex Orekhov <alex.everget161@gmail.com> (https://github.com/everget)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/everget/rowser/issues"
  },
  "homepage": "https://github.com/everget/rowser#readme",
  "dependencies": {},
  "devDependencies": {
    "benchmark": "^2.1.4",
    "bowser": "^1.7.3",
    "chai": "^4.1.2",
    "cloc": "^2.2.0",
    "eslint": "^4.7.2",
    "istanbul": "^0.4.5",
    "lodash.random": "^3.2.0",
    "mobile-detect": "^1.3.7",
    "mocha": "^3.5.3",
    "opn": "^5.1.0",
    "platform": "^1.3.4",
    "rollup": "^0.50.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-uglify": "^2.0.1",
    "sinon": "^3.2.1",
    "ua-parser-js": "^0.7.14",
    "uglify-es": "^3.1.1"
  }
}
